{
  "version": 2,
  "name": "deal-protocol",
  "builds": [
    { "src": "api/server.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/server.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "cleanUrls": true
}
