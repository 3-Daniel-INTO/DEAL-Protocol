{
  "version": 2,
  "cleanUrls": true,
  "framework": null,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "dest": "/api/$1" },
    { "source": "/(.*)", "dest": "/index.html" }
  ]
}
