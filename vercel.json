{
  "version": 2,
  "name": "deal-protocol",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Sovereign-Platform", "value": "DEAL-DALabs" },
        { "key": "X-Neural-Engine", "value": "MIA-X" },
        { "key": "Cache-Control", "value": "public, max-age=3600, must-revalidate" }
      ]
    }
  ]
}
