{
  "version": 2,
  "name": "deal-sovereign",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Sovereign-Engine", "value": "MIA-X" },
        { "key": "X-Intelligence-Layer", "value": "G-AGI" },
        { "key": "Content-Security-Policy", "value": "default-src 'self' https: 'unsafe-inline' 'unsafe-eval';" }
      ]
    }
  ]
}
