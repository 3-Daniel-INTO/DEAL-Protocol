{
  "version": 2,
  "name": "deal-protocol",
  "cleanUrls": true,
  "buildCommand": null,
  "installCommand": "echo 'MIA-X: Bypass Installation'",
  "outputDirectory": ".",
  "public": true,
  "rewrites": [
    { "source": "/api/(.*)", "dest": "https://deal-protocol.onrender.com/api/$1" },
    { "source": "/(.*)", "dest": "/index.html" }
  ]
}